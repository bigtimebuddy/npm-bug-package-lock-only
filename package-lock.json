{
  "name": "npm-bug",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-bug",
      "license": "MIT",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/@package/a": {
      "resolved": "packages/package-a",
      "link": true
    },
    "node_modules/@package/b": {
      "resolved": "packages/package-b",
      "link": true
    },
    "node_modules/@tools/a": {
      "resolved": "packages/tools-a",
      "link": true
    },
    "packages/package-a": {
      "name": "@package/a",
      "version": "1.0.0",
      "license": "MIT",
      "devDependencies": {
        "@tools/a": "1.0.0"
      }
    },
    "packages/package-b": {
      "name": "@package/b",
      "version": "1.0.0",
      "license": "MIT",
      "devDependencies": {
        "@tools/a": "1.0.0"
      }
    },
    "packages/tools-a": {
      "name": "@tools/a",
      "version": "1.0.0",
      "license": "MIT"
    }
  },
  "dependencies": {
    "@package/a": {
      "version": "file:packages/package-a",
      "requires": {
        "@tools/a": "1.0.0"
      }
    },
    "@package/b": {
      "version": "file:packages/package-b",
      "requires": {
        "@tools/a": "1.0.0"
      }
    },
    "@tools/a": {
      "version": "file:packages/tools-a"
    }
  }
}
